{
  "name": "roommates-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "NODE_ENV=development tsc-watch -p tsconfig.build.json --onSuccess \"node dist/src/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/src/main.js\"",
    "start:prod": "NODE_ENV=production node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:create": "typeorm migration:create -n custom-name",
    "migration:up": "typeorm migration:run",
    "migration:down": "typeorm migration:revert"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "@material-ui/core": "^4.11.0",
    "@nestjs/common": "^6.11.5",
    "@nestjs/config": "^0.1.0",
    "@nestjs/core": "^6.11.5",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^7.5.4",
    "@nestjs/typeorm": "^6.1.3",
    "@react-ssr/core": "^0.24.0",
    "@react-ssr/nestjs-express": "^0.24.0",
    "@sendgrid/mail": "^7.3.0",
    "@sentry/node": "^5.27.3",
    "@types/dotenv": "^6.1.1",
    "aws-sdk": "^2.690.0",
    "axios": "^0.21.0",
    "bcrypt": "^3.0.6",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "dotenv": "^8.0.0",
    "formik": "^2.2.5",
    "i18next": "^19.8.5",
    "ioredis": "^4.19.2",
    "mysql": "^2.17.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.5",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "tslint-react": "^5.0.0",
    "typeorm": "^0.2.18",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@nestjs/testing": "6.1.1",
    "@types/express": "4.16.1",
    "@types/hapi__joi": "^16.0.9",
    "@types/jest": "24.0.11",
    "@types/node": "^14.0.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/supertest": "2.0.7",
    "jest": "24.7.1",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.5.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
